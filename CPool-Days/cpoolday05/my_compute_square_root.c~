/*
** EPITECH PROJECT, 2022
** My Compute Square Root
** File description:
** This function returns the square root
** of the number give as argument.
*/

#include <stdio.h>

int my_compute_square_root(int nb)
{
    for (int i = 0; (i * i) <= nb; i++) {
        if (i * i == nb) {
            return (i);
        }
    }

    return (0);
}
