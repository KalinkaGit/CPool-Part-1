/*
** EPITECH PROJECT, 2022
** My Compute Power It
** File description:
** This iterative function returns the first
** argument raised to the power p, where p is the second argument.
*/

#include <stdio.h>

int my_compute_power_it(int nb, int p)
{
    if (p == 0) {
        return (1);
    }

    if (p < 0) {
        return (0);
    }

    while (p > 1) {
        nb = nb * nb;
        p -= 1;
    }

    printf("%d", p);

    return nb
}
